{
  "inbounds": [
    {
      "port": 1080, // 监听端口
      "listen": "0.0.0.0",
      "protocol": "socks", // 入口协议为 SOCKS 5
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls"]
      },
      "settings": {
        "auth": "noauth"  //socks的认证设置，noauth 代表不认证，由于 socks 通常在客户端使用，所以这里不认证
      }
    }
  ],
  "outbounds": [  //出站连接用于向远程网站或下一级代理服务器发送数据， 比如这里就是配置定向到下一级代理。
                  //所以这里 client/config.json 的 outbound 要与 server/config.json 的 inbound 能 match
    {
      "protocol": "vmess", // 出口协议
      "settings": {
        "vnext": [
          {
            "address": "23.254.164.117", // 服务器地址，请修改为你自己的服务器 IP 或域名
            "port": 31415,  // 服务器端口
            "users": [
              {
                "id": "DE1F24AE-3642-9D66-AF94-951F06CF3590",  // 用户 ID，必须与服务器端配置相同
                "alterId": 32 // 此处的值也应当与服务器相同
              }
            ]
          }
        ]
      }
    },
     {
      "protocol": "freedom",
      "settings": {},
      "tag": "direct"
    }
  ],
     
  "routing": {
        "domainStrategy": "IPOnDemand",
        "rules": [
            {
                "type": "field",
                "ip": [
                    "geoip:private"
                ],
                "outboundTag": "direct"
            }
        ]
    },

  "transport": {
    "kcpSettings": {
      "mtu": 1350,
      "tti": 20,
      "uplinkCapacity": 5,
      "downlinkCapacity": 20,
      "congestion": true,
      "readBufferSize": 2,
      "writeBufferSize": 2,
      "header": {
        "type": "dtls"
      }
    }
  }


}